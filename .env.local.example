# API配置
NEXT_PUBLIC_API_URL=https://api.example.com
NEXT_PUBLIC_API_KEY=your_api_key_here

# 认证
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_here

# 外部服务
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
NEXT_PUBLIC_SENTRY_DSN=https://xxx@xxx.ingest.sentry.io/xxx

# 数据库
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# 存储
NEXT_PUBLIC_STORAGE_URL=https://storage.example.com
STORAGE_ACCESS_KEY=your_access_key_here
STORAGE_SECRET_KEY=your_secret_key_here

# 缓存
REDIS_URL=redis://localhost:6379

# 主题
NEXT_PUBLIC_DEFAULT_THEME=light

# 功能标志
NEXT_PUBLIC_FEATURE_NEW_DASHBOARD=true
NEXT_PUBLIC_FEATURE_ADVANCED_SEARCH=false

# PWA配置
NEXT_PUBLIC_SITE_URL=https://example.com 